{
  "name": "mcp-map-server",
  "version": "0.1.0",
  "description": "Universal MCP server for mapping services with support for Google Maps, OpenStreetMap, and RiskScape APIs",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "mcp-map-server": "dist/cli.js"
  },
  "files": [
    "dist",
    "dist/**/*.map",
    "README.md"
  ],
  "scripts": {
    "build": "tsup --dts",
    "start": "node dist/cli.js",
    "dev": "cross-env NODE_ENV=development tsup --watch",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "map",
    "mapping",
    "api",
    "google-maps",
    "openstreetmap",
    "riskscape",
    "llm",
    "typescript",
    "mcp",
    "server",
    "streamable",
    "location",
    "geocoding",
    "navigation",
    "osm"
  ],
  "author": "Leon F Nortje",
  "license": "MIT",
  "homepage": "https://github.com/LeonFNortje/mcp-map-server#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeonFNortje/mcp-map-server.git"
  },
  "bugs": {
    "url": "https://github.com/LeonFNortje/mcp-map-server/issues"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.4.0",
    "@modelcontextprotocol/sdk": "^1.11.0",
    "@types/yargs": "^17.0.33",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "yargs": "^17.7.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.17.0",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.5.0",
    "ts-jest": "^29.2.5",
    "tsup": "^8.5.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "overrides": {
    "glob": "^10.0.0",
    "inflight": "npm:@isaacs/inflight@^1.0.1",
    "source-map": "^0.7.4"
  }
}
